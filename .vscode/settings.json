{
    "files.exclude": 
    {
        "**/*.rpyc": true,
        "**/*.rpa": true,
        "**/*.rpymc": true,
        "**/*.gen.rpy": true,
        "**/*.bak": true,
        "**/cache/": true
    },
    "editor.tabSize": 4,
    "explorer.sortOrder": "type",
    "emeraldwalk.runonsave": 
    {
        "commands": [
            {
                "match": "\\.py$",
                "cmd": "mypy ${file}"
            },
            /*
            {
                "match": ".*",
                "cmd": "build.bat"
            }
            */
        ]
    }
}